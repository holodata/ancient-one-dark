  "tokenColors": [
    {
      "name": "Invalid - some grammar use it for showing obvious syntax errors.",
      "scope": "invalid",
      "settings": {
        "foreground": "#f070a3"
      }
    },
    {
      "name": "Reset string interpolation (template literal) having the wrong color bug",
      "scope": [
        "meta.embedded",
        "source.groovy.embedded",
        "meta.template.expression"
      ],
      "settings": {
        "foreground": "#d6cfdb"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "punctuation.definition.variable",
        "support.variable.property.dom",
        "support.variable.dom",
        "support.variable.property",
        "punctuation.separator.parameter",
        "string.interpolated.pug variable"
      ],
      "settings": {
        "foreground": "#9d8eff"
      }
    },
    {
      "name": "Function parameter",
      "scope": [
        "variable.parameter",
        "parameter.variable",
        "meta.function.parameter variable",
        "source.rust meta.type_params.rust"
      ],
      "settings": {
        "foreground": "#9d8eff"
      }
    },
    {
      "name": "Function name, class name, new class expression.",
      "scope": [
        "entity.name.function",
        "support.class",
        "support.function",
        "new.expr entity.name.type",
        "entity.other.inherited-class"
      ],
      "settings": {
        "foreground": "#c776dd"
      }
    },
    {
      "name": "Keyword",
      "scope": ["keyword", "constant.language.import-export-all"],
      "settings": {
        "foreground": "#bfbbcd"
      }
    },
    {
      "name": "Keyword (import), operator (+-=)",
      "scope": [
        "keyword.control",
        "keyword.operator",
        "storage",
        "support.type",
        "keyword.operator.expression",
        "keyword.operator.new"
      ],
      "settings": {
        "foreground": "#bfbbcd"
      }
    },
    {
      "name": "Types",
      "scope": [
        "entity.name.type",
        "support.type.primitive",
        "support.type.builtin",
        "meta.type.annotation entity.name.type",
        "meta.type.parameters entity.name.type"
      ],
      "settings": {
        "foreground": "#6eace5"
      }
    },
    {
      "name": "Comments",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#797782"
      }
    },
    {
      "name": "Strings",
      "scope": ["string", "punctuation.definition.string"],
      "settings": {
        "foreground": "#c9a974"
      }
    },
    {
      "name": "Escape character (\n, \t ...)",
      "scope": "constant.character.escape",
      "settings": {
        "foreground": "#bba5d4"
      }
    },
    {
      "name": "Punctuation, string template symbol, keyword type",
      "scope": [
        "punctuation.definition.typeparameters",
        "keyword.operator.type",
        "keyword.operator.optional",
        "punctuation.definition.template-expression",
        "source.tsx punctuation.section.embedded",
        "source.jsx punctuation.section.embedded"
      ],
      "settings": {
        "foreground": "#6C607A"
      }
    },
    {
      "name": "Constant",
      "scope": "constant",
      "settings": {
        "foreground": "#b573b4"
      }
    },
    {
      "name": "Constant (numeric) (language, for instance, NaN, undefined, Infinity for JS/TS),",
      "scope": ["constant.numeric", "constant.language"],
      "settings": {
        "foreground": "#ca616f"
      }
    },
    {
      "name": "Punctuation",
      "scope": ["punctuation", "meta.brace"],
      "settings": {
        "foreground": "#98939b"
      }
    },
    {
      "name": "this/self",
      "scope": ["variable.language.this", "variable.language.special.self"],
      "settings": {
        "foreground": "#bba5d4"
      }
    },
    {
      "name": "[JS] JSDoc type",
      "scope": "comment.block.documentation entity.name.type",
      "settings": {
        "foreground": "#6C607A"
      }
    },
    {
      "name": "[JS] super() call",
      "scope": "variable.language.super",
      "settings": {
        "foreground": "#bcaee5"
      }
    },
    {
      "name": "[HTML] Doctype tag",
      "scope": [
        "meta.tag.metadata.doctype entity.name.tag",
        "meta.tag.metadata.doctype punctuation.definition.tag",
        "meta.tag.metadata.doctype string",
        "meta.tag.metadata.doctype entity.other.attribute-name.html",
        "meta.tag.sgml.doctype"
      ],
      "settings": {
        "foreground": "#98939b"
      }
    },
    {
      "name": "[HTML] tag name",
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#bba5d4"
      }
    },
    {
      "name": "[HTML] String inside tag (attribute name)",
      "scope": "meta.tag string",
      "settings": {
        "foreground": "#FBD089"
      }
    },
    {
      "name": "[HTML] tag attribute name",
      "scope": [
        "meta.tag entity.other.attribute-name",
        "entity.other.attribute-name.html"
      ],
      "settings": {
        "foreground": "#8c76c0"
      }
    },
    {
      "name": "[HTML] special entity ( &nbsp; &gt; )",
      "scope": ["constant.character.entity", "punctuation.definition.entity"],
      "settings": {
        "foreground": "#6B71C4"
      }
    },
    {
      "name": "[Markdown] Header",
      "scope": ["entity.name.section.markdown", "markup.heading.setext"],
      "settings": {
        "foreground": "#a289dc"
      }
    },
    {
      "name": "[Markdown] List symbol (-, +, *, 1. ...)",
      "scope": "punctuation.definition.list",
      "settings": {
        "foreground": "#8c76c0"
      }
    },
    {
      "name": "[Markdown] separator ---",
      "scope": "meta.separator.markdown",
      "settings": {
        "foreground": "#8c76c0"
      }
    },
    {
      "name": "[Markdown] inline code `text`",
      "scope": "markup.inline.raw",
      "settings": {
        "foreground": "#bba5d4"
      }
    },
    {
      "name": "[Markdown] bold text **bold**",
      "scope": "markup.bold",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "[Markdown] italic _italic_",
      "scope": "markup.italic",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "[Markdown] link description [description](...)",
      "scope": [
        "meta.link punctuation.definition.string",
        "meta.image punctuation.definition.string"
      ],
      "settings": {
        "foreground": "#98939b"
      }
    },
    {
      "name": "[Markdown] link itself [...](LINK)",
      "scope": [
        "link",
        "markup.underline.link",
        "constant.other.reference.link.markdown"
      ],
      "settings": {
        "foreground": "#bcaee5"
      }
    },
    {
      "name": "[Markdown] blockquote text (>)",
      "scope": "markup.quote",
      "settings": {
        "foreground": "#6B71C4"
      }
    },
    {
      "name": "[CSS] Tag and asterisk",
      "scope": ["entity.name.tag.css", "entity.name.tag.wildcard"],
      "settings": {
        "foreground": "#FBD089"
      }
    },
    {
      "name": "[CSS] class (.class)",
      "scope": [
        "entity.other.attribute-name.class",
        "entity.other.attribute-name punctuation.definition.entity"
      ],
      "settings": {
        "foreground": "#8c76c0"
      }
    },
    {
      "name": "[CSS] pseudo class/element :before :hover",
      "scope": [
        "entity.other.attribute-name.pseudo-element",
        "entity.other.attribute-name.pseudo-class",
        "constant.other.color"
      ],
      "settings": {
        "foreground": "#bba5d4"
      }
    },
    {
      "name": "[CSS] id (#id)",
      "scope": [
        "entity.other.attribute-name.id",
        "entity.other.attribute-name.id punctuation.definition.entity"
      ],
      "settings": {
        "foreground": "#bcaee5"
      }
    },
    {
      "name": "[CSS] numbers",
      "scope": [
        "source.css constant.numeric",
        "source.less constant.numeric",
        "source.scss constant.numeric"
      ],
      "settings": {
        "foreground": "#FBD089"
      }
    },
    {
      "name": "[CSS] property (background-color)",
      "scope": ["meta.property-name", "support.type.property-name"],
      "settings": {
        "foreground": "#6B71C4"
      }
    },
    {
      "name": "[Css] property value",
      "scope": [
        "meta.property-value",
        "meta.property-value constant.other",
        "support.constant.property-value"
      ],
      "settings": {
        "foreground": "#bcaee5"
      }
    },
    {
      "name": "[CSS] url",
      "scope": "variable.parameter.url",
      "settings": {
        "foreground": "#6B71C4"
      }
    },
    {
      "name": "[PHP] php script symbols <?php ?>",
      "scope": [
        "punctuation.section.embedded.begin.php",
        "punctuation.section.embedded.end.php"
      ],
      "settings": {
        "foreground": "#8c76c0"
      }
    },
    {
      "name": "[ini] Header",
      "scope": "entity.name.section",
      "settings": {
        "foreground": "#bcaee5"
      }
    },
    {
      "name": "[JSON] property name",
      "scope": "support.type.property-name.json",
      "settings": {
        "foreground": "#967bd6"
      }
    },
    {
      "name": "[Diff] Inserted",
      "scope": "markup.inserted",
      "settings": {
        "foreground": "#96bd65"
      }
    },
    {
      "name": "[Diff] Modified",
      "scope": "markup.changed",
      "settings": {
        "foreground": "#e0bc58"
      }
    },
    {
      "name": "[Diff] Deleted",
      "scope": "markup.deleted",
      "settings": {
        "foreground": "#d36666"
      }
    },
    {
      "name": "[Diff] Header",
      "scope": "meta.diff.header",
      "settings": {
        "foreground": "#375de6"
      }
    },
    {
      "name": "[Diff] Range",
      "scope": "meta.diff.range",
      "settings": {
        "foreground": "#bba5d4"
      }
    },
    {
      "name": "Italicize",
      "scope": [
        // "keyword.control",
        "quote",
        "storage",
        "this",
        "comment",
        "punctuation.definition.comment",
        "modifier"
      ],
      "settings": {
        "fontStyle": "italic"
      }
    }
  ]
}
